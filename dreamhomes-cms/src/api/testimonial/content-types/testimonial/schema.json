{
    "kind": "collectionType",
    "collectionName": "testimonials",
    "info": {
        "singularName": "testimonial",
        "pluralName": "testimonials",
        "displayName": "Testimonial",
        "description": "Client testimonials"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "clientName": {
            "type": "string",
            "required": true
        },
        "location": {
            "type": "string"
        },
        "quote": {
            "type": "text",
            "required": true
        },
        "rating": {
            "type": "integer",
            "min": 1,
            "max": 5,
            "default": 5
        },
        "photo": {
            "type": "media",
            "multiple": false,
            "required": false,
            "allowedTypes": [
                "images"
            ]
        }
    }
}